<UserControl
    x:Class="Prism_EventAggregator.Views.AdminView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Prism_EventAggregator.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Width="600"
    Height="400"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <Button
            Width="120"
            Height="30"
            Command="{Binding SubscribeCommand}"
            Content="Subscibe Msg" />
        <Button
            Width="120"
            Height="30"
            Command="{Binding SubValidateCommand}"
            Content="Subscibe Msg" />

        <Button
            Width="120"
            Height="30"
            Command="{Binding CancelSubscribeCommand}"
            Content="Cancel Subscribe" />

    </StackPanel>
</UserControl>
